
public abstract class Account 
{
	protected Identity identity;
	
	public Account(String name, Image image)
	{
		
	}
	
	public boolean equals(Object object)
	{
		if(this.getClass().getName().equals(object.getClass().getName()))
		{
			return true;
		}
		else
		{
			return false;
		}
	}
}
