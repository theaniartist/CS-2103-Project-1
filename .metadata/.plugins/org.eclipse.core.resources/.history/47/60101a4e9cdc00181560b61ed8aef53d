
public class Possession extends Account
{
	private Person owner; //HOW TO ABSTRACT getOwner();
	private double price;
	
	public Possession(String name, Image image, Person owner, double price)
	{
		super(name, image);
		this.owner = owner;
		this.price = price;
	}
	
	public double getPrice()
	{
		return price;
	}
}
