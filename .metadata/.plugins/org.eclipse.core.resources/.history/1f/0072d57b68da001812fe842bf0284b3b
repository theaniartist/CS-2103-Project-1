
public class Identity
{
	private String name;
	private Image image;
	
	public Identity (String name, Image image) //Constructor 
	{
		this.name = name;
		this.image = image;
	}
	
	public String getName()
	{
		return name;
	}

	
	public boolean equals(Identity o)
	{
		if(name.equals(o.getName())) //TODO
		{
			return true;
		}
		else
		{
			return false;
		}
	}
}
